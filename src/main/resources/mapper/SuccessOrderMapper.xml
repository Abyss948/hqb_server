<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hqb.mapper.SuccessOrderMapper">

    <select id="getSuccessOrderByProvideid" resultType="Success" parameterType="int">
        select * from success where provideid =#{provideid};
    </select>

    <select id="getSuccessOrderByNeedid" resultType="Success" parameterType="int">
        select * from success where needid =#{needid};
    </select>


</mapper>